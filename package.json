{
  "name": "tuning-movie",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev:stack:agent-monitor",
    "dev:backend": "npm run dev --workspace=apps/backend",
    "dev:frontend": "npm run dev --workspace=apps/frontend",
    "dev:agent": "bash scripts/run-tuning-agent-typescript.sh",
    "dev:monitor": "npm run dev --workspace=apps/agent-monitor",
    "dev:agent-test": "npm run dev --workspace=apps/agent-test",
    "dev:stack:system": "node scripts/dev-orchestrator.mjs system",
    "dev:stack:agent": "node scripts/dev-orchestrator.mjs agent",
    "dev:stack:agent-monitor": "node scripts/dev-orchestrator.mjs agent_monitor",
    "dev:stack:all": "node scripts/dev-orchestrator.mjs all",
    "build": "npm run build --workspaces",
    "build:backend": "npm run build --workspace=apps/backend",
    "build:frontend": "npm run build --workspace=apps/frontend",
    "build:agent": "npm run build --workspace=apps/tuning-agent-typescript",
    "build:monitor": "npm run build --workspace=apps/agent-monitor",
    "build:agent-test": "npm run build --workspace=apps/agent-test",
    "start:agent": "bash scripts/run-tuning-agent-typescript.sh start",
    "db:seed": "npm run db:seed --workspace=apps/backend"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
